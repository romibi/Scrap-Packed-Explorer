<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="NormalForeground" Color="White"/>
    <SolidColorBrush x:Key="ListViewForeground" Color="#FF174AD0" />
    <SolidColorBrush x:Key="NormalBackground" Color="Black"/>
    <SolidColorBrush x:Key="SplitterBackground" Color="#FF212121"/>
    <SolidColorBrush x:Key="DisabledBackground" Color="#FF373737"/>
    <SolidColorBrush x:Key="NormalBorderBrush" Color="Gray" />

    <SolidColorBrush x:Key="Button.MouseOver.Background" Color="#FF022940"/>
    <SolidColorBrush x:Key="Button.MouseOver.Border" Color="#FF5091c3"/>
    <SolidColorBrush x:Key="Button.Pressed.Background" Color="#FF09293a"/>
    <SolidColorBrush x:Key="Button.Pressed.Border" Color="#FF73aad3"/>
    <SolidColorBrush x:Key="Button.Disabled.Background" Color="#FF0a0a0a"/>
    <SolidColorBrush x:Key="Button.Disabled.Border" Color="#FF4b5053"/>
    <SolidColorBrush x:Key="Button.Disabled.Foreground" Color="#FF7d7d7d"/>
</ResourceDictionary>